{
  "name": "numberjack",
  "version": "0.1.3",
  "description": "Read number for human to understand easier",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "prebuild": "rm -rf dist",
    "release": "npx standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/colus001/numberjack.git"
  },
  "bugs": {
    "url": "https://github.com/colus001/numberjack/issues"
  },
  "keywords": [
    "number",
    "human-readable",
    "read"
  ],
  "author": "Seokjun Kim <colus001@me.com>",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/numeral": "^0.0.29",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "numeral": "^2.0.6"
  }
}
